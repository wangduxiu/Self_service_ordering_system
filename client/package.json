{
  "name": "angular-template",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "yarn & yarn create-test-environment & ng serve",
    "build": "yarn & yarn create-test-environment & ng build",
    "start-prod": "yarn & yarn create-prod-environment & ng serve --prod",
    "build-prod": "yarn & yarn create-prod-environment & ng build --prod",
    "test": "ng test",
    "test:watch": "yarn run test --watch",
    "lint": "ng lint",
    "prettify": "prettier --write \"./src/app/**/*.{ts,js,css,json}\"",
    "prettify-environment": "prettier --write src/environments/environment.ts",
    "e2e": "ng e2e",
    "precommit": "yarn lint-staged",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "mock-server": "yarn json-server --watch src/app/shared/mockdata/db.json",
    "create-test-environment": "node src/createEnvironment.js test && yarn prettify-environment",
    "create-qas-environment": "node src/createEnvironment.js qas && yarn prettify-environment",
    "create-prod-environment": "node src/createEnvironment.js prod && yarn prettify-environment"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/animations": "7.2.7",
    "@angular/common": "7.2.7",
    "@angular/compiler": "7.2.7",
    "@angular/core": "7.2.7",
    "@angular/forms": "7.2.7",
    "@angular/http": "7.2.7",
    "@angular/platform-browser": "7.2.7",
    "@angular/platform-browser-dynamic": "7.2.7",
    "@angular/platform-server": "7.2.7",
    "@angular/router": "7.2.7",
    "@angular/service-worker": "7.2.7",
    "@ngrx/router-store": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "@ngrx/store-devtools": "^7.3.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^6.4.0",
    "tassign": "^1.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.3.1",
    "@angular/cli": "7.3.3",
    "@angular/compiler-cli": "7.2.7",
    "@angular/language-service": "7.2.7",
    "@angular/service-worker": "7.2.7",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jest": "^24.0.9",
    "@types/node": "~11.9.5",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "json-server": "^0.14.2",
    "lint-staged": "^8.1.4",
    "prettier": "1.16.4",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.2",
    "tslint": "~5.13.0",
    "typescript": "~3.2.4"
  }
}
